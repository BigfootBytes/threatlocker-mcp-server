  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "key",
        "value": "Authorization",
        "type": "string"
      },
      {
        "key": "value",
        "value": "{{apiKey}}",
        "type": "string"
      },
      {
        "key": "in",
        "value": "header",
        "type": "string"
      }
    ]
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "pm.request.headers.add({",
          "    key: 'ManagedOrganizationId',",
          "    value: pm.environment.get('orgId')",
          "});",
          "pm.request.headers.add({",
          "    key: 'OverrideManagedOrganizationId',",
          "    value: pm.environment.get('orgId')",
          "});"
        ]
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://betaportalapi.g.threatlocker.com/portalapi"
    }
  ],
